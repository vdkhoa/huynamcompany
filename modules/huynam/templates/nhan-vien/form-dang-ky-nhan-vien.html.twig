{#
/**
 * @file
 * 
 */
#}
{{ form.form_build_id }}
{{ form.form_token }}
{{ form.form_id }}
{% spaceless %}
  <div class="row">
    <div class="col-md-3">
     <label class="control-label form-required">Mã Nhân Viên:</label>
    </div>
    <div class="col-md-6">
     {{ form.ma_nhan_vien }}
    </div>
  </div>
  <div class="row">
    <div class="col-md-3">
     <label class="control-label form-required">Họ Tên Nhân Viên:</label>
    </div>
    <div class="col-md-6">
     {{ form.ho_ten_nhan_vien }}
    </div>
  </div>
  <div class="row">
    <div class="col-md-3">
     <label class="control-label">Địa Chỉ Nhân Viên:</label>
    </div>
    <div class="col-md-6">
     {{ form.dia_chi_nhan_vien }}
    </div>
  </div>
  <div class="row">
    <div class="col-md-3">
     <label class="control-label">Năm Sinh:</label>
    </div>
    <div class="col-md-6">
     {{ form.ngay_sinh_nhan_vien }}
    </div>
  </div>
  <div class="row">
    <div class="col-md-3">
     <label class="control-label">Giới Tính:</label>
    </div>
    <div class="col-md-6">
     {{ form.gioi_tinh_nhan_vien }}
    </div>
  </div>
  <div class="row">
    <div class="col-md-3">
     <label class="control-label">Số Điện Thoại:</label>
    </div>
    <div class="col-md-6">
     {{ form.so_dien_thoai_nhan_vien }}
    </div>
  </div>
  <div class="row">
    <div class="col-md-3">
     <label class="control-label">Chức Vụ:</label>
    </div>
    <div class="col-md-6">
     {{ form.chuc_vu_nhan_vien }}
    </div>
  </div>
  <div class="row">
    <div class="col-md-3"></div>
    <div class="col-md-6">
      {{ form.submit }}
    </div>
  </div>
{% endspaceless %}

{#
/**
 * @file
 * 
 */
#}
{{ form.form_build_id }}
{{ form.form_token }}
{{ form.form_id }}
{% spaceless %}
  <div class="row">
    <div class="col-md-3">
      <label class="control-label form-required">Mã Khách Hàng:</label>
    </div>
    <div class="col-md-6">
      {{ form.ma_khach_hang }}
    </div>
  </div>
  <div class="row">
    <div class="col-md-3">
      <label class="control-label form-required">Họ Tên Khách Hàng:</label>
    </div>
    <div class="col-md-6">
      {{ form.ten_khach_hang }}
    </div>
  </div>
  <div class="row">
    <div class="col-md-3">
      <label class="control-label">Địa Chỉ Khách Hàng:</label>
    </div>
    <div class="col-md-6">
      {{ form.dia_chi_khach_hang }}
    </div>
  </div>
  <div class="row">
    <div class="col-md-3">
      <label class="control-label">Người Đại Diện:</label>
    </div>
    <div class="col-md-6">
      {{ form.nguoi_dai_dien }}
    </div>
  </div>
  <div class="row">
    <div class="col-md-3">
      <label class="control-label">Số Điện Thoại:</label>
    </div>
    <div class="col-md-6">
      {{ form.so_dien_thoai_khach_hang }}
    </div>
  </div>
  <div class="row">
    <div class="col-md-3">
      <label class="control-label">Công Nợ:</label>
    </div>
    <div class="col-md-6">
      {{ form.loai_cong_no }}
    </div>
  </div>
  <div class="row">
    <div class="col-md-3">
      <label class="control-label">Loại Khách Hàng:</label>
    </div>
    <div class="col-md-6">
      {{ form.loai_khach_hang }}
    </div>
  </div>
  <div class="row">
    <div class="col-md-3">
    </div>
    <div class="col-md-9">
      {{ form.submit }}
      {{ form.cancel }}
    </div>
  </div>
{% endspaceless %}

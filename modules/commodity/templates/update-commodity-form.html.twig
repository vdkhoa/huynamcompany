{#
/**
 * @file
 * 
 */
#}
{{ form.form_build_id }}
{{ form.form_token }}
{{ form.form_id }}
{% spaceless %}
  <div class="row">
    <div class="col-md-3">
     <label class="control-label form-required">Mã Hàng Hóa:</label>
    </div>
    <div class="col-md-6">
     {{ form.commodity_code }}
    </div>
  </div>
  <div class="row">
    <div class="col-md-3">
     <label class="control-label form-required">Tên Hàng Hóa:</label>
    </div>
    <div class="col-md-6">
     {{ form.commodity_name }}
    </div>
  </div>
  <div class="row">
    <div class="col-md-3">
     <label class="control-label">Đơn Giá:</label>
    </div>
    <div class="col-md-6">
     {{ form.commodity_price }}
    </div>
  </div>
  <div class="row">
    <div class="col-md-3">
    </div>
    <div class="col-md-9">
      {{ form.submit }}
    </div>
  </div>
{% endspaceless %}
